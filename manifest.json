{
    "manifest_version": 2,
    "name": "DONOTSHOWTITLE",
    "description": "一个快乐上网的插件",
    "version": "1.0.0",
    "permissions": [
        "https://secure.flickr.com/"
    ],
    "content_scripts": [
        {
            "matches": ["https://www.zhihu.com/question/*", "http://www.zhihu.com/question/*", "https://www.zhihu.com/*", "https://www.v2ex.com/*", "https://cnodejs.org/"],
            "css": [
                "custom.css"
            ],
            "run_at": "document_start"
        }
    ],
    "browser_action": {
        "default_icon": "icon.png"
    }
}